<?xml version="1.0" encoding="utf-8" ?>
<!-- this is a data node for application wide settings --> 
<component name="AppSettingsNode" extends="Node">
    <interface>
        <!-- tiles are squares -->
        <field id="tile_size" type="integer" value="30" />
        <field id="room_min_width" type="integer" value="10" />
        <field id="room_min_height" type="integer" value="8" />
        <field id="room_max_width" type="integer" value="20" />
        <field id="room_max_height" type="integer" value="10" />
	</interface> 
</component>


